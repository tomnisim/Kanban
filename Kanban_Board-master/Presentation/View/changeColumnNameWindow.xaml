<Window x:Class="Presentation.View.changeColumnNameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Presentation.View"
        mc:Ignorable="d"
        Title="changeColumnNameWindow" Height="202.552" Width="260.209">
    <Grid>
        <Button Content="OK" HorizontalAlignment="Left" Margin="79,98,0,0" VerticalAlignment="Top" Width="75" Click="changeColumnName_Click"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="59,66,0,0" TextWrapping="Wrap" Text="{Binding Path=Name, Mode=TwoWay}" VerticalAlignment="Top" Width="120"/>
        <Label Content="Enter column name" HorizontalAlignment="Left" Margin="59,35,0,0" VerticalAlignment="Top"/>
        <Label Content="{Binding Path=Message, Mode=OneWay}" Foreground="Red" HorizontalAlignment="Left" Margin="109,129,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
